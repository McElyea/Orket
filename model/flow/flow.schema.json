{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Flow",
  "type": "object",
  "required": ["name", "venue", "band", "score"],
  "properties": {
    "name": {
      "type": "string",
      "description": "Unique flow name."
    },
    "description": {
      "type": "string",
      "description": "Optional human-readable description."
    },
    "venue": {
      "type": "string",
      "description": "Path to a Venue JSON file."
    },
    "band": {
      "type": "string",
      "description": "Path to a Band JSON file."
    },
    "score": {
      "type": "string",
      "description": "Path to a Score JSON file."
    }
  },
  "additionalProperties": false
}
